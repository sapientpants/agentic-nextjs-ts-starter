{
  "name": "agentic-nextjs-ts-starter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:workflows": "./scripts/actionlint.sh",
    "lint:markdown": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "lint:markdown:fix": "markdownlint-cli2 --fix \"**/*.md\" \"#node_modules\"",
    "lint:yaml": "yamllint '**/*.{yml,yaml}' --ignore='node_modules/**' --ignore='.github/workflows/**' --ignore='pnpm-lock.yaml'",
    "lint:spelling": "cspell lint --no-progress '**'",
    "deps:circular": "madge --circular --extensions ts,tsx --ts-config tsconfig.json app/",
    "deps:graph": "madge app/ --ts-config tsconfig.json --image reports/deps-graph.png",
    "deps:summary": "madge --summary app/ --ts-config tsconfig.json",
    "deps:check": "ncu",
    "deps:update:minor": "ncu -u --target minor",
    "deps:update:patch": "ncu -u --target patch",
    "deps:cruise": "depcruise app --config .dependency-cruiser.js",
    "deps:cruise:graph": "depcruise app --include-only '^app' --output-type dot | dot -T svg > dependency-graph.svg",
    "deps:unused": "depcheck",
    "license:check": "license-checker --summary",
    "license:report": "license-checker --json --out reports/licenses.json",
    "license:csv": "license-checker --csv --out reports/licenses.csv",
    "deps:update:latest": "ncu -u --target latest",
    "dead-code": "knip",
    "duplication": "jscpd app/",
    "mutation-test": "stryker run",
    "mutation-test:incremental": "stryker run --incremental",
    "size": "size-limit",
    "perf:bench": "autocannon -c 100 -d 10 http://localhost:3000",
    "publint": "publint",
    "attw": "attw --pack .",
    "ts-prune": "ts-prune",
    "pkg:validate": "pnpm publint && pnpm attw",
    "docs:generate": "typedoc",
    "docs:api": "pnpm build && api-extractor run --local",
    "metrics": "pnpm deps:summary && pnpm deps:circular && pnpm duplication",
    "metrics:update": "node scripts/extract-quality-metrics.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --reporter=verbose --coverage",
    "test:coverage:watch": "vitest --coverage",
    "coverage:report": "vitest run --coverage --reporter=verbose",
    "coverage:open": "open coverage/index.html",
    "clean": "rimraf .next coverage",
    "reset": "pnpm clean && pnpm install",
    "quick-check": "pnpm typecheck && pnpm lint && pnpm test",
    "precommit": "pnpm audit --audit-level critical && pnpm format && pnpm lint:spelling && pnpm lint:yaml && pnpm lint:markdown && pnpm lint:workflows && pnpm typecheck && pnpm lint && pnpm deps:circular && pnpm deps:cruise && pnpm duplication && pnpm dead-code && pnpm ts-prune && pnpm test:coverage",
    "setup": "./scripts/setup.sh",
    "lint-staged": "lint-staged",
    "sbom": "pnpm dlx @cyclonedx/cdxgen -o sbom.cdx.json",
    "scan:container": "./scripts/scan-container.sh",
    "scan:container:sarif": "./scripts/scan-container.sh --format sarif --output container-scan.sarif",
    "scan:secrets": "./scripts/scan-secrets.sh",
    "scan:secrets:protect": "./scripts/scan-secrets.sh protect",
    "scan:secrets:history": "./scripts/scan-secrets.sh history",
    "scan:secrets:report": "./scripts/scan-secrets.sh report",
    "prepare": "husky",
    "release": "changeset version && pnpm build",
    "release:publish": "pnpm build && changeset publish",
    "release:tag": "git add -A && git commit -m \"chore(release): version packages\" && git tag -a v$(node -p \"require('./package.json').version\") -m \"Release\"",
    "commit": "git-cz",
    "changeset": "changeset",
    "changeset:status": "changeset status --since=main",
    "ci:local": "./scripts/ci-local.sh",
    "ci:local:fast": "./scripts/ci-local.sh --fast",
    "doctor": "node -e \"console.log('Node:', process.version); console.log('npm scripts:', Object.keys(require('./package.json').scripts).length, 'available'); console.log('Run: pnpm run help')\"",
    "help": "node -e \"const s=require('./package.json').scripts; console.log('Available commands:'); Object.keys(s).sort().forEach(k => console.log('  pnpm run ' + k.padEnd(20) + ' # ' + s[k].split(' ')[0]))\""
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,json,jsonc,json5}": [
      "eslint --fix"
    ],
    "*.md": [
      "markdownlint-cli2 --fix"
    ],
    "*.{yml,yaml}": [
      "yamllint"
    ]
  },
  "pnpm": {
    "overrides": {
      "tar": ">=7.5.2",
      "tmp": ">=0.2.4"
    }
  },
  "dependencies": {
    "next": "16.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@cyclonedx/cdxgen": "11.11.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@fast-check/vitest": "^0.2.3",
    "@microsoft/api-extractor": "7.55.0",
    "@next/eslint-plugin-next": "16.0.3",
    "@size-limit/preset-small-lib": "11.2.0",
    "@stryker-mutator/core": "9.3.0",
    "@stryker-mutator/typescript-checker": "9.3.0",
    "@stryker-mutator/vitest-runner": "9.3.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "4.0.9",
    "autocannon": "8.0.0",
    "changelog-github-custom": "1.2.7",
    "commitizen": "4.3.1",
    "cspell": "9.3.2",
    "cz-conventional-changelog": "3.3.0",
    "depcheck": "1.4.7",
    "dependency-cruiser": "17.2.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.2.1",
    "eslint-plugin-jsonc": "2.21.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-no-barrel-files": "1.2.2",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-regexp": "2.10.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-unicorn": "62.0.0",
    "fast-check": "^4.3.0",
    "husky": "^9.1.7",
    "jscpd": "4.0.5",
    "jsdom": "27.2.0",
    "jsonc-eslint-parser": "2.4.1",
    "knip": "5.69.1",
    "license-checker": "25.0.1",
    "lint-staged": "^16.2.6",
    "madge": "8.0.0",
    "markdownlint-cli2": "0.19.0",
    "npm-check-updates": "19.1.2",
    "prettier": "^3.6.2",
    "publint": "0.3.15",
    "rimraf": "^6.1.0",
    "size-limit": "11.2.0",
    "ts-prune": "0.10.3",
    "typedoc": "0.28.14",
    "typescript": "^5.9.3",
    "validate-branch-name": "1.3.2",
    "vite": "7.2.2",
    "vitest": "^4.0.9",
    "yaml-lint": "1.7.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
