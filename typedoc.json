{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "plugin": [],
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "readme": "README.md",
  "hideGenerator": false,
  "includeVersion": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Constructor",
    "Property",
    "Method",
    "Accessor",
    "TypeAlias",
    "Interface",
    "Enum",
    "Variable"
  ],
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "logLevel": "Info"
}
